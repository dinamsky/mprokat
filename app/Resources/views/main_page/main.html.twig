{# begin блок слайдера ТОП оплаченных #}

<div class="uk-width-1-1" style="padding: 10px 0 15px 0; background: #eaeaea;"> 
<div class="standard_wide">
    <div class="owl-carousel owl-carousel-all owl-theme uk-width-1-1" data-dots="0" data-items="4">
        {% for card in top10Slider %}
            {{ include('search/one_grid_view.html.twig', {'card':card, 'height':'250px', 'micro':true}) }}
        {% endfor %}
    </div>
    <div class="uk-text-center">
        {% if app.session.has('logged_user') %}
            <a class="" href="/card/new" title="Хотите сюда?<br>Разместите объявление!" uk-tooltip>{% trans %}Разместить объявление в ТОП{% endtrans %}</a>
        {% else %}
            <a class="after_top" title="Хотите сюда?<br>Разместите объявление!" uk-toggle="target: #sign_in_or_up" uk-tooltip>{% trans %}Разместить объявление в ТОП{% endtrans %}</a>
        {% endif %}
    </div>
</div>
</div>

{# end блок слайдера ТОП оплаченных #}

